{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["es5", "es6", "dom"],
    "module": "commonjs",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "allowJs": true,
    "jsx": "react",
    "paths": {
      "*": ["node_modules/*"]
    }
  },
  "exclude": ["node_modules"]

}

